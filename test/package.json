{
  "name": "bedrock-vue-vc-test",
  "version": "0.0.1-0",
  "type": "module",
  "private": true,
  "description": "Vue.js credential card component for Bedrock Web apps test",
  "browser": {
    ".": "./components/index.js"
  },
  "scripts": {
    "start": "node --preserve-symlinks test.js",
    "test": "node --preserve-symlinks test.js test --framework karma",
    "test:debug": "node --preserve-symlinks test.js test --framework karma --log-level debug",
    "coverage": "cross-env NODE_ENV=test c8 npm run test",
    "coverage-ci": "cross-env NODE_ENV=test c8 --reporter=lcovonly --reporter=text-summary --reporter=text npm run test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/digitalbazaar/bedrock-vue-vc"
  },
  "keywords": [
    "bedrock"
  ],
  "author": {
    "name": "Digital Bazaar, Inc.",
    "email": "support@digitalbazaar.com",
    "url": "https://digitalbazaar.com"
  },
  "bugs": {
    "url": "https://github.com/digitalbazaar/bedrock-vue-vc/issues"
  },
  "homepage": "https://github.com/digitalbazaar/bedrock-vue-vc",
  "dependencies": {
    "@bedrock/core": "^6.0.1",
    "@bedrock/express": "^8.0.0",
    "@bedrock/karma": "^6.0.0",
    "@bedrock/quasar": "^8.0.1",
    "@bedrock/server": "^5.0.0",
    "@bedrock/test": "^8.0.5",
    "@bedrock/views": "^10.0.3",
    "@bedrock/vue": "^4.0.0",
    "@bedrock/vue-vc": "file:..",
    "@bedrock/web": "^2.0.1",
    "@bedrock/web-fontawesome": "^2.0.0",
    "@bedrock/webpack": "^8.0.0",
    "c8": "^7.11.3",
    "cross-env": "^7.0.3",
    "vue": "^3.2.36",
    "vue-router": "^4.0.15"
  },
  "devDependencies": {
    "eslint": "^7.32.0",
    "eslint-config-digitalbazaar": "^2.8.0",
    "eslint-plugin-jsdoc": "^37.9.7",
    "eslint-plugin-vue": "^8.6.0",
    "jsdoc": "^3.6.4",
    "jsdoc-to-markdown": "^7.1.1"
  },
  "c8": {
    "excludeNodeModules": false,
    "include": [
      "node_modules/@bedrock/vue-vc/**"
    ],
    "exclude": [
      "node_modules/@bedrock/vue-vc/node_modules/**"
    ],
    "reporter": [
      "lcov",
      "text-summary",
      "text"
    ]
  }
}
