  },
  "homepage": "https://github.com/digitalbazaar/bedrock-vue-vc",
  "dependencies": {
    "@bedrock/core": "^6.0.1",
    "@bedrock/express": "^8.2.0",
    "@bedrock/karma": "^6.0.0",
    "@bedrock/quasar": "^10.0.0",
    "@bedrock/server": "^5.0.0",
    "@bedrock/test": "^8.0.5",
